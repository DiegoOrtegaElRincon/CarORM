package com.diego.vehiculo.entity.models;

@Entity
@Table(name = "seguros_vehiculos")
public class SeguroVehiculo {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;
    
    @OneToOne
    @JoinColumn(name = "vehiculo_id")
    private Vehiculo vehiculo;
    
